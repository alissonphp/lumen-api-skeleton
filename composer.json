{
    "name": "alissonphp/lumen-api-skeleton",
    "description": "Lumen API skeleton with JWT to manager tokens, Socialite to OAuth Providers, MongoDB driver and Predis to Redis cache storage.",
    "keywords": ["lumen", "api", "jwt", "oauth", "socialite", "mongodb"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "jenssegers/mongodb": "^3.2",
        "laravel/socialite": "^3.0",
        "tymon/jwt-auth": "1.0.0-beta.3",
        "predis/predis": "^1.1",
        "illuminate/redis": "^5.4",
        "filp/whoops": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "appzcoder/lumen-routes-list": "*",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
